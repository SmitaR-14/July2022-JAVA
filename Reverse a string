import java.io.*;
import java.util.*;

class reverseAstring {
    public static void main(String[] args)
    {
        Scanner sc = new Scanner (System.in);        
        String str = sc.next();          
          for(int j=str.length()-1;j>=0;j--){
               System.out.print(str.charAt(j));
          }
      
    }
}
